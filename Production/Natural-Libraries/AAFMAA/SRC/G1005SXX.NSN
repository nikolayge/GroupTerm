IF #IAS-USER AND #SCR-COMMAND = 'RL'   /* RE2
  MOVE TRUE TO #VALID                  /* RE2
END-IF                                 /* RE2
*
IF   (NOT #VALID)
    OR ((TYPE-OF-INPUT = 'N')
    AND #SAS-USER
    AND (NOT #COMMAND-SECURITY-CODE = 1 OR = 3 OR = 5 OR = 9 OR = 99
    OR = 6       OR = 95 OR = 96))
