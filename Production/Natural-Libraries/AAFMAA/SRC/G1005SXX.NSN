    OR ((TYPE-OF-INPUT = 'O' OR = 'P' OR = 'Q' OR = 'R') /* ID OPTIONAL
    AND (#SCR-ID = 0)
    AND #IAS-USER AND (#COMMAND-SECURITY-CODE = 1 OR = 3))
