* * * * * * * * * * * * * * * * *
DEFINE SUBROUTINE MEMBER-SECURITY
* * * * * * * * * * * * * * * * *
*
IF   (TYPE-OF-INPUT = 'C' OR = 'Q')   /* IM number reqd or optional  /* KEEP
    AND (#SCR-ID LT 100000) AND (#SCR-ID NE 0)
  ADD 700000 TO #SCR-ID
END-IF
*
RESET #FOUND-ID
*
IF  ((#SCR-ID GE 700000) /* IM NUMBER                                /* KEEP
    AND (#SCR-ID LT 800000))
*
*  IF #SAS-USER                                /* DW16
*    MOVE 'Intro Member access not valid for AFSC user' TO #ERR-MSG
*    ESCAPE ROUTINE
*  END-IF
*
  MOVE TRUE TO #FOUND-ID
  FIND (1) IM-V WITH ID-NUMBER = #SCR-ID
    IF NO RECORDS FOUND
      RESET #FOUND-ID
    END-NOREC
  END-FIND
*
  IF (TYPE-OF-INPUT = 'C') OR (#SCR-ID NE 0)
    IF NOT #FOUND-ID
      MOVE 'IM number required or not found' TO #ERR-MSG
    END-IF
  ELSE
    RESET #SCR-ID
  END-IF
*
  ESCAPE ROUTINE
END-IF
*
IF   (#SCR-ID GE 500000) /* AT NUMBER             /* KEEP
    AND (#SCR-ID LT 600000)
  FIND (1) AT-V WITH ID-NUMBER = #SCR-ID
    IF NO RECORDS FOUND
      MOVE 'AT number entered not found' TO #ERR-MSG
    END-NOREC
  END-FIND
*
*  IF #SAS-USER AND (AT-V.INTRO-FLAG NE 'V')            /* DW16
*    MOVE 'Only AFSC "AT" recs are accessable to AFSC' TO #ERR-MSG
*  END-IF
*
*  IF #IAS-USER AND (AT-V.INTRO-FLAG = 'V')                  /* DW16
*    MOVE 'AFSC "AT" recs are inaccessable to AAFMAA' TO #ERR-MSG
*    ESCAPE ROUTINE
*  END-IF
*
  IF   (#SCR-COMMAND = 'ST')
      AND (AT-V.MEMBER-CN NE ' ')
    MOVE AT-V.MEMBER-CN-N TO #SCR-ID  /* Use Linked CN number from AT record
  ELSE
*                            /* AT is accessible to all
    ESCAPE ROUTINE
  END-IF
END-IF
*
RESET #VALID #FOUND-ID #FOUND-ST
IF #SCR-ID GT 999999 /* SSN                          /* KEEP
  IF #SCR-COMMAND = 'IM' OR = 'IC' OR = 'ID'
    FIND IM-V WITH SSN = #SCR-ID
      MOVE TRUE TO #FOUND-ID
      IF #IAS-USER                /* DW16
*       OR #99-USER
        MOVE TRUE TO #VALID
        ESCAPE ROUTINE
      END-IF
    END-FIND
  END-IF
*
  IF #SCR-COMMAND = 'AT' OR = 'NM' OR = 'AI'
      OR = 'AA' OR = 'PO' OR = 'MR' OR = 'ML' OR = 'AN' OR = 'AE'
    FIND AT-V WITH SSN = #SCR-ID
      MOVE TRUE TO #FOUND-ID
*
*      IF #SAS-USER AND (#SCR-COMMAND = 'AT')
*       AND (#SCR-SUFFIX = 'COM')  /* create afsc combo "at" from aafmaa
*       AND (AT-V.INTRO-FLAG NE 'V')
*        MOVE TRUE TO #VALID
*        ESCAPE BOTTOM
*      END-IF
*
      IF  #SAS-USER AND (AT-V.INTRO-FLAG NOT = 'V')
        ESCAPE TOP
      END-IF
*
      IF #IAS-USER AND (AT-V.INTRO-FLAG = 'V')
        ESCAPE TOP
      END-IF
*
      MOVE TRUE TO #VALID
      ESCAPE BOTTOM
    END-FIND
  END-IF
*
* DISPLAY #FOUND-ST #VALID #GF-MBR #INS-MBR #SVC-MBR #IAS-USER #SCR-ID
  FIND ST-V WITH SSN = #SCR-ID                      /* KEEP
    IF   (PROCESS-IND = 'C')   /* to bypass trusts of members
        AND (INTRO-FLAG NE 'A')
      ESCAPE TOP
    END-IF
*
    MOVE TRUE TO #FOUND-ID #FOUND-ST
*
*     IF #SAS-USER AND (#SCR-COMMAND = 'AT')   /* DW16
*      AND (#SCR-SUFFIX = 'COM')  /* create afsc combo "at" from aafmaa
*      AND (ST-V.PROCESS-IND = 'I' OR = 'O' OR = 'D') /* ins-only
*       MOVE TRUE TO #VALID  /* Ins-only/opt-out/de-grandfathered
*       ESCAPE BOTTOM
*     END-IF
*
*    PERFORM CHECK-MBR-SECURITY     /* DW16
*
*   IF NOT #VALID
*       RESET #SVC-MBR #INS-MBR #GF-MBR
*     ELSE
*       ESCAPE BOTTOM
*     END-IF
  END-FIND
* DISPLAY #FOUND-ST #VALID #GF-MBR #INS-MBR #SVC-MBR #IAS-USER #SCR-ID
*         #FOUND-ID
  IF NOT #FOUND-ID
    ESCAPE ROUTINE
  END-IF
*
* DISPLAY #FOUND-ST #VALID #GF-MBR #INS-MBR #SVC-MBR #IAS-USER
*  IF NOT #VALID   /* REmoved 9/17/2009  DW99
*    IF #IAS-USER
*      MOVE 'AAFMAA not allowed access to AFSC members' TO #ERR-MSG
*    ELSE
*      IF #SAS-USER
*        IF NOT #SCR-COMMAND = 'DI' OR = 'NS' OR = 'NU' OR = 'AD'
*                         OR = 'DA' OR = 'DD' OR = 'TB'    /* DW3
*          MOVE 'AFSC not allowed access to AAFMAA members until death'
*                                                      TO #ERR-MSG
*        END-IF
*      ELSE
*        MOVE 'Access to this member not valid for this user'
*           TO #ERR-MSG
*      END-IF
*    END-IF
*  END-IF
*
  IF  #INS-MBR AND #FOUND-ST     /* DW6
      AND (PROCESS-IND = 'C')    /* Contact Entity or Pending AT
      AND (#ERR-MSG = ' ')
      AND (NOT #SCR-COMMAND = 'NM' OR = 'CG' OR = 'AD' OR = 'NU'
      OR = 'RL' OR = 'NS' OR = 'CR' OR = 'TR' OR = 'CA' OR= 'TA' or = 'TC')       /* PM17  vt1
* MOVE 'Command Invalid for Contact Entity' TO #ERR-MSG
    MOVE 'Command Invalid for Non-Insured' TO #ERR-MSG
    IF PROCESS-IND = 'C'                                                 /* PM1
        AND ST-V.INTRO-FLAG = 'A'                                        /* PM1
        AND #SCR-COMMAND = 'AT' OR = 'ST'  OR = 'CB' OR = 'CA' OR = 'FA' /* PM2/DW12
      RESET #ERR-MSG                                                     /* PM1
    END-IF                                                               /* PM1
  END-IF
*
  ESCAPE ROUTINE
END-IF
*
MOVE TRUE TO #FOUND-ST
RESET #VALID
FIND (1) ST-V WITH MEMBER-KEY = #SCR-ID-6
  IF NO RECORDS FOUND
    FIND (1) ST-V WITH WIDOW-KEY = #SCR-ID-6
      IF NO RECORD FOUND
        RESET #FOUND-ST
      END-NOREC
    END-FIND
  END-NOREC
END-FIND
*
* IF #FOUND-ST /* if no find let g1000pxx reject it
*  PERFORM CHECK-MBR-SECURITY
* DISPLAY #FOUND-ST #VALID #GF-MBR #INS-MBR #SVC-MBR #IAS-USER
*
*  IF NOT #VALID
*    IF #IAS-USER
*      MOVE 'AAFMAA not allowed access to AFSC Members' TO #ERR-MSG
*    ELSE
*      IF #SAS-USER
*        IF NOT #SCR-COMMAND = 'DI' OR = 'NS' OR = 'NU' OR = 'AD'
*                         OR = 'DA' OR = 'DD'
*          MOVE 'AFSC not allowed access to AAFMAA Members until death'
*                                                         TO #ERR-MSG
*        END-IF
*      ELSE
*        MOVE 'Access to this Member not valid for this User'
*            TO #ERR-MSG
*      END-IF
*    END-IF
*  END-IF
* END-IF
*
* IF  #INS-MBR AND #FOUND-ST
IF (PROCESS-IND = 'C')    /* Contact Entity or Pending AT
    AND (#ERR-MSG = ' ')
    AND (NOT #SCR-COMMAND = 'NM' OR = 'CG' OR = 'AD' OR = 'NU'
    OR = 'RL' OR = 'NS' OR = 'CR' OR = 'TR' OR = 'CA' OR= 'TA' or = 'TC' )       /* PM17  /* vt1
* MOVE 'Command Invalid for Contact Entity' TO #ERR-MSG
  MOVE 'Command Invalid for Non-Insured' TO #ERR-MSG
  IF PROCESS-IND = 'C'                                                  /* PM1
      AND ST-V.INTRO-FLAG = 'A'                                         /* PM1
      AND #SCR-COMMAND = 'AT' OR = 'ST'  OR = 'CB' OR = 'CA' OR = 'FA'  /* PM2/DW12
    RESET #ERR-MSG                                                      /* PM1
  END-IF                                                                /* PM1
END-IF
*
END-SUBROUTINE
