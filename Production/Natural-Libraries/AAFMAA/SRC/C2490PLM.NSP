* PGM-ID: C2490PLM     TRANSFER LOAN from 1 pol to another
DEFINE DATA
GLOBAL USING BCOMMGXX WITH MASTER-BLOCK
LOCAL USING GONERLXX      /* ERROR ROUTINE FIELDS
LOCAL
*
1 ST-V VIEW OF A-STATUS
 2 ID-NUMBER
 2 PROCESS-IND
 2 INTRO-FLAG
 2 DATE-ACCT-UPDATE
 2 ONLINE-ACTION-GN
 2 NAME
 2 X-INTEREST-RATE       (9)
 2 X-PRINCIPAL-CHG-CODE  (9)
 2 X-LOAN-DATE           (9)
 2 X-LOAN-AMOUNT         (9)
 2 X-ACCUM-INTEREST      (9)
 2 X-NXT-YR-ACCUM-INT    (9)
 2 X-BILL-INTEREST-AMT   (9)
 2 X-CAP-AMT             (9)
 2 X-OLD-LOAN-AMT        (9)
 2 X-INTEREST-RECEIVED   (9)
 2 X-ALT-LP-IND          (9)
 2 X-ALT-LP-AMT          (9)
 2 X-BILL-SW             (9)
 2 X-LPO-PMT-RCVD        (9)
 2 X-LPO-PRIOR-LOAN-BAL  (9)
 2 X-LPO-PRIOR-ACC-INT   (9)
*
 2 X-LOAN-DATE-2         (9)
 2 X-LOAN-AMOUNT-2       (9)
 2 X-ACCUM-INTEREST-2    (9)
 2 X-NXT-YR-ACCUM-INT-2  (9)
 2 X-BILL-INTEREST-AMT-2 (9)
 2 X-CAP-AMT-2           (9)
 2 X-OLD-LOAN-AMT-2      (9)
 2 X-INTEREST-RECEIVED-2 (9)
 2 X-ALT-LP-IND-2        (9)
 2 X-ALT-LP-AMT-2        (9)
 2 X-BILL-SW-2           (9)
 2 X-LPO-PMT-RCVD-2      (9)
 2 X-LPO-PRIOR-LOAN-BAL-2(9)
 2 X-LPO-PRIOR-ACC-INT-2 (9)
*
 2 STATUS             (9)
 2 PLAN               (9)
 2 ISSUE-DATE         (9)
 2 CONV-DURATION      (9)
 2 CASH-VALUE         (9)
*
1 TR-V VIEW OF A-TRANSACTION-REGISTER
 2 ID-NUMBER         (N6)
 2 PROCESS-IND       (A1)
 2 NAME              (A25)
 2 DATE-LAST-UPDATE  (N8)
 2 TERMINAL-ID       (A8)
 2 CLERK-ID          (A3)
 2 TRANSACTION-DATN  (P9)
 2 TRANSACTION-TIMN  (P7)
 2 OLD-DATA          (A125/20)
 2 NEW-DATA          (A125/20)
*
1 #TR-REG-LN-TRANS
 2 #TR-REG-LT-LBL-1         (A17) INIT <'LM-LOAN-TRANSFER '>
 2 #TR-REG-LT-LBL-2         (A11) INIT <'ISSUE-DATE '>
 2 #TR-REG-LT-ISSUE-DATE    (N08)
 2 #TR-REG-LT-LBL-3         (A14) INIT <' FIX LOAN AMT '>
 2 #TR-REG-LT-FX-LOAN-AMT   (A09)
 2 #TR-REG-LT-LBL-4         (A13) INIT <' VAR LON AMT '>
 2 #TR-REG-LT-VR-LOAN-AMT   (A10)
1 REDEFINE #TR-REG-LN-TRANS
 2 #TR-REG-LOAN-TRANS       (A82)
*
1 #YYYYMMDD  (N8)
1 REDEFINE #YYYYMMDD
 2 #YYYYMM (N6)
 2 REDEFINE #YYYYMM
  3 #YYYY   (N4)
  3 #MM     (N2)
 2 #DD     (N2)
1 REDEFINE #YYYYMMDD
 2 #YYYY-A (A4)
 2 #MM-A   (A2)
 2 #DD-A   (A2)
1 REDEFINE #YYYYMMDD
 2 #YYYYMMDD-A (A8)
*
1 #TRN-DT-FROM    (A8)
1 REDEFINE #TRN-DT-FROM
 2 #TRN-DT-FROM-MM   (A2)
 2 #TRN-DT-FROM-DD   (A2)
 2 #TRN-DT-FROM-YYYY (A4)
1 #TRN-DT-TO      (A8)
1 REDEFINE #TRN-DT-TO
 2 #TRN-DT-TO-MM   (A2)
 2 #TRN-DT-TO-DD   (A2)
 2 #TRN-DT-TO-YYYY (A4)
*
1 #INT           (I2)
1 #NOTE-TEXT     (A60/10)
1 #LOAN-A       (A11)
1 #ISS-DT-FROM  (N8)
1 #ISS-DT-FROM-A  (A10)
1 #ISS-DT-TO    (N8)
1 #ISS-DT-TO-A    (A10)
1 #FR-X    (I2)
1 #TO-X    (I2)
1 #I1      (I2)
1 #I2      (I2)
1 #ISN     (P8)
1 #ITR1    (I2)
1 #GEN-CV  (C)
1 #SECURITY-ERROR (L)
1 #PLAN-FROM  (A3)
1 #PLAN-TO    (A3)
1 #PLAN-FROM-LBL   (A25)
1 #PLAN-TO-LBL     (A25)
1 #MATCHING-ISS-DT  (L)
1 #MATCHING-ISS-DT-CV (C)
1 #ORIG-FIXED   (N7.2)
1 #ORIG-VAR     (N7.2)
*
END-DEFINE
*
INCLUDE GONERCXX
*
FIND (1) ST-V WITH MEMBER-KEY = #CGA-ID-G
  IF NO RECORDS FOUND
    INPUT TEXT 'ID not found' USING MAP 'G1000MXX'
    END TRANSACTION
    FETCH 'G1000PXX'
  END-NOREC
*
  CALLNAT 'GSECURXX'
   ST-V.ID-NUMBER
   #CGA-CLERK
   #SECURITY-ERROR
*
  IF #SECURITY-ERROR
    INPUT TEXT 'Security Violation - Member is linked to user'
      USING MAP 'G1000MXX'
    END TRANSACTION
    FETCH 'G1000PXX'
  END-IF
*
  MOVE *ISN TO #ISN
END-FIND
*
MOVE (AD=P) TO #MATCHING-ISS-DT-CV
MOVE '*' TO #CGA-SCR-COMMAND
INPUT MARK *#TRN-DT-FROM USING MAP 'C2490MLM'
*
IF #CGA-SCR-COMMAND NE '*'
  FETCH 'G1000PXX'
END-IF
*
* IF NOT INTRO-FLAG = 'C' OR = 'B' OR = 'P' OR = 'Q' /* removed 5/25/04
*   REINPUT 'Must be a CAP member'
*   MARK *#CGA-SCR-ID
* END-IF
*
IF #TRN-DT-FROM NE MASK (MMDDYYYY)
  REINPUT 'Date format error (MMDDYYYY)'
  MARK *#TRN-DT-FROM
END-IF
*
IF #TRN-DT-TO NE MASK (MMDDYYYY)
  REINPUT 'Date format error (MMDDYYYY)'
  MARK *#TRN-DT-TO
END-IF
*
COMPRESS #TRN-DT-FROM-YYYY #TRN-DT-FROM-MM #TRN-DT-FROM-DD
    INTO #YYYYMMDD-A LEAVING NO
MOVE #YYYYMMDD TO #ISS-DT-FROM
*
COMPRESS #TRN-DT-TO-YYYY #TRN-DT-TO-MM #TRN-DT-TO-DD
    INTO #YYYYMMDD-A LEAVING NO
MOVE #YYYYMMDD TO #ISS-DT-TO
*
RESET #MATCHING-ISS-DT
IF #ISS-DT-FROM = #ISS-DT-TO
  MOVE TRUE TO #MATCHING-ISS-DT
  RESET #MATCHING-ISS-DT-CV
  MOVE 'Enter plan of from policy' TO #PLAN-FROM-LBL
  MOVE 'Enter plan of to policy  ' TO #PLAN-TO-LBL
*
  MOVE '*' TO #CGA-SCR-COMMAND
  INPUT MARK *#TRN-DT-FROM USING MAP 'C2490MLM'
*
  IF #CGA-SCR-COMMAND NE '*'
    FETCH 'G1000PXX'
  END-IF
*
  IF #PLAN-FROM = #PLAN-TO
    REINPUT 'From & To Plan must not equal'
    MARK *#PLAN-FROM
  END-IF
END-IF
*
FOR #I1 1 TO 9
  IF  ((#ISS-DT-FROM = ISSUE-DATE (#I1)) AND NOT #MATCHING-ISS-DT)
   OR (#MATCHING-ISS-DT
    AND (#ISS-DT-FROM = ISSUE-DATE (#I1)) AND (#PLAN-FROM = PLAN (#I1)))
    MOVE #I1 TO #FR-X
  END-IF
*
  IF  ((#ISS-DT-TO = ISSUE-DATE (#I1)) AND NOT #MATCHING-ISS-DT)
   OR (#MATCHING-ISS-DT
    AND (#ISS-DT-TO = ISSUE-DATE (#I1)) AND (#PLAN-TO = PLAN (#I1)))
    MOVE #I1 TO #TO-X
  END-IF
END-FOR
*
IF #FR-X = 0
  REINPUT 'Transfer from Issue Date Policy not found'
  MARK *#TRN-DT-FROM
END-IF
*
IF #TO-X = 0
  REINPUT 'Transfer to Issue Date Policy not found'
  MARK *#TRN-DT-TO
END-IF
*
IF NOT STATUS (#FR-X) = 'E' OR = 'D'
* IF STATUS (#FR-X) NE 'E'   /* removed 5/25/2004
  REINPUT 'Transfer from policy must be active or terminated this month'
  MARK *#TRN-DT-FROM
END-IF
*
IF STATUS (#TO-X) NE 'D'
  REINPUT 'Transfer to policy must be an active policy'
  MARK *#TRN-DT-TO
END-IF
*
IF   (X-LOAN-AMOUNT    (#FR-X) = 0)
 AND (X-ACCUM-INTEREST (#FR-X) = 0)
 AND (X-LOAN-DATE      (#FR-X) = 0)
 AND (X-LOAN-AMOUNT-2    (#FR-X) = 0)
 AND (X-ACCUM-INTEREST-2 (#FR-X) = 0)
 AND (X-LOAN-DATE-2      (#FR-X) = 0)
  REINPUT 'No loan(s) to transfer'
  MARK *#TRN-DT-FROM
END-IF
*
* IF CONV-DURATION (#FR-X) NE 999    /* removed 5/25/04
*   REINPUT 'Transfer from policy must ba a CAP policy'
*   MARK *#TRN-DT-FROM
* END-IF
*
IF   (X-LOAN-AMOUNT (#FR-X) NE 0)
 AND (X-LOAN-AMOUNT (#TO-X) NE 0)
 AND (X-INTEREST-RATE (#FR-X) NE X-INTEREST-RATE (#TO-X))
  REINPUT 'Fixed Interest rates do not match'
  MARK *#TRN-DT-FROM
END-IF
*
IF (CONV-DURATION (#FR-X) NE 999) 
 AND  (X-LOAN-AMOUNT   (#FR-X) + X-ACCUM-INTEREST (#FR-X) 
     + X-LOAN-AMOUNT-2 (#FR-X) + X-ACCUM-INTEREST-2 (#FR-X) 
     + X-LOAN-AMOUNT   (#TO-X) + X-ACCUM-INTEREST (#TO-X) 
     + X-LOAN-AMOUNT-2 (#TO-X) + X-ACCUM-INTEREST-2 (#TO-X)) 
        GT (CASH-VALUE (#TO-X) * .75) 
* Loan Value is really 75% of Cash Value not CSV but I think this is OK 
  REINPUT 'Total new Loan/Int will exceed 75% of cash-surr-value' 
  MARK *#TRN-DT-FROM 
END-IF 
*
IF X-ALT-LP-IND (#FR-X) = ' '
  MOVE '0' TO X-ALT-LP-IND (#FR-X)
END-IF
*
IF X-ALT-LP-IND-2 (#FR-X) = ' '
  MOVE '0' TO X-ALT-LP-IND-2 (#FR-X)
END-IF
*
IF X-ALT-LP-IND (#TO-X) = ' '
  MOVE '0' TO X-ALT-LP-IND (#TO-X)
END-IF
*
IF X-ALT-LP-IND-2 (#TO-X) = ' '
  MOVE '0' TO X-ALT-LP-IND-2 (#TO-X)
END-IF
*
IF  ((NOT X-ALT-LP-IND   (#TO-X) = '0' OR = ' ')
     AND (X-ALT-LP-IND   (#FR-X) NE X-ALT-LP-IND   (#TO-X)))
 OR ((NOT X-ALT-LP-IND-2 (#TO-X) = '0' OR = ' ')
     AND (X-ALT-LP-IND-2 (#FR-X) NE X-ALT-LP-IND-2 (#TO-X)))
  REINPUT 'ALP Indicators do not match'
  MARK *#TRN-DT-FROM
END-IF
*
* IF  (X-LOAN-AMOUNT    (#TO-X) NE 0)    /* removed 5/25/04
*  OR (X-ACCUM-INTEREST (#TO-X) NE 0)
* OR (X-LOAN-DATE      (#TO-X) NE 0)  /* removed 5/30/2002 eac
*   REINPUT 'Loan already exists on policy to transfer to'
*   MARK *#TRN-DT-TO
* END-IF
*
GET ST-V #ISN
*
MOVE X-LOAN-AMOUNT   (#FR-X) TO #ORIG-FIXED
MOVE X-LOAN-AMOUNT-2 (#FR-X) TO #ORIG-VAR
*
IF CONV-DURATION (#FR-X) = 999
  MOVE 999 TO CONV-DURATION (#TO-X)
  RESET       CONV-DURATION (#FR-X)
END-IF
*
IF X-LOAN-AMOUNT (#TO-X) NE 0
  MOVE '4' TO X-PRINCIPAL-CHG-CODE (#FR-X)
ELSE
  MOVE X-PRINCIPAL-CHG-CODE  (#FR-X)
    TO X-PRINCIPAL-CHG-CODE  (#TO-X)
END-IF
*
MOVE X-INTEREST-RATE       (#FR-X)
  TO X-INTEREST-RATE       (#TO-X)
*
IF X-LOAN-DATE (#TO-X) = 0
  MOVE X-LOAN-DATE (#FR-X) TO X-LOAN-DATE (#TO-X)
END-IF
*
ADD  X-LOAN-AMOUNT         (#FR-X)
  TO X-LOAN-AMOUNT         (#TO-X)
ADD  X-ACCUM-INTEREST      (#FR-X)
  TO X-ACCUM-INTEREST      (#TO-X)
ADD  X-NXT-YR-ACCUM-INT    (#FR-X)
  TO X-NXT-YR-ACCUM-INT    (#TO-X)
ADD  X-BILL-INTEREST-AMT   (#FR-X)
  TO X-BILL-INTEREST-AMT   (#TO-X)
ADD  X-CAP-AMT             (#FR-X)
  TO X-CAP-AMT             (#TO-X)
ADD  X-OLD-LOAN-AMT        (#FR-X)
  TO X-OLD-LOAN-AMT        (#TO-X)
ADD  X-INTEREST-RECEIVED   (#FR-X)
  TO X-INTEREST-RECEIVED   (#TO-X)
MOVE X-ALT-LP-IND          (#FR-X)
  TO X-ALT-LP-IND          (#TO-X)
ADD  X-ALT-LP-AMT          (#FR-X)
  TO X-ALT-LP-AMT          (#TO-X)
MOVE X-BILL-SW             (#FR-X)
  TO X-BILL-SW             (#TO-X)
ADD  X-LPO-PMT-RCVD        (#FR-X)
  TO X-LPO-PMT-RCVD        (#TO-X)
ADD  X-LPO-PRIOR-LOAN-BAL  (#FR-X)
  TO X-LPO-PRIOR-LOAN-BAL  (#TO-X)
ADD  X-LPO-PRIOR-ACC-INT   (#FR-X)
  TO X-LPO-PRIOR-ACC-INT   (#TO-X)
*
IF X-LOAN-DATE-2 (#TO-X) = 0
  MOVE X-LOAN-DATE-2 (#FR-X) TO X-LOAN-DATE-2 (#TO-X)
END-IF
*
ADD  X-LOAN-AMOUNT-2       (#FR-X)
  TO X-LOAN-AMOUNT-2       (#TO-X)
ADD  X-ACCUM-INTEREST-2    (#FR-X)
  TO X-ACCUM-INTEREST-2    (#TO-X)
ADD  X-NXT-YR-ACCUM-INT-2  (#FR-X)
  TO X-NXT-YR-ACCUM-INT-2  (#TO-X)
ADD  X-BILL-INTEREST-AMT-2 (#FR-X)
  TO X-BILL-INTEREST-AMT-2 (#TO-X)
ADD  X-CAP-AMT-2           (#FR-X)
  TO X-CAP-AMT-2           (#TO-X)
ADD  X-OLD-LOAN-AMT-2      (#FR-X)
  TO X-OLD-LOAN-AMT-2      (#TO-X)
ADD  X-INTEREST-RECEIVED-2 (#FR-X)
  TO X-INTEREST-RECEIVED-2 (#TO-X)
MOVE X-ALT-LP-IND-2        (#FR-X)
  TO X-ALT-LP-IND-2        (#TO-X)
ADD  X-ALT-LP-AMT-2        (#FR-X)
  TO X-ALT-LP-AMT-2        (#TO-X)
MOVE X-BILL-SW-2           (#FR-X)
  TO X-BILL-SW-2           (#TO-X)
ADD  X-LPO-PMT-RCVD-2      (#FR-X)
  TO X-LPO-PMT-RCVD-2      (#TO-X)
ADD  X-LPO-PRIOR-LOAN-BAL-2(#FR-X)
  TO X-LPO-PRIOR-LOAN-BAL-2(#TO-X)
ADD  X-LPO-PRIOR-ACC-INT-2 (#FR-X)
  TO X-LPO-PRIOR-ACC-INT-2 (#TO-X)
*
RESET X-INTEREST-RATE       (#FR-X)
RESET X-PRINCIPAL-CHG-CODE  (#FR-X)
RESET X-LOAN-DATE           (#FR-X)
RESET X-LOAN-AMOUNT         (#FR-X)
RESET X-ACCUM-INTEREST      (#FR-X)
RESET X-NXT-YR-ACCUM-INT    (#FR-X)
RESET X-BILL-INTEREST-AMT   (#FR-X)
RESET X-CAP-AMT             (#FR-X)
RESET X-OLD-LOAN-AMT        (#FR-X)
RESET X-INTEREST-RECEIVED   (#FR-X)
RESET X-ALT-LP-IND          (#FR-X)
RESET X-ALT-LP-AMT          (#FR-X)
RESET X-BILL-SW             (#FR-X)
RESET X-LPO-PMT-RCVD        (#FR-X)
RESET X-LPO-PRIOR-LOAN-BAL  (#FR-X)
RESET X-LPO-PRIOR-ACC-INT   (#FR-X)
*
RESET X-LOAN-DATE-2         (#FR-X)
RESET X-LOAN-AMOUNT-2       (#FR-X)
RESET X-ACCUM-INTEREST-2    (#FR-X)
RESET X-NXT-YR-ACCUM-INT-2  (#FR-X)
RESET X-BILL-INTEREST-AMT-2 (#FR-X)
RESET X-CAP-AMT-2           (#FR-X)
RESET X-OLD-LOAN-AMT-2      (#FR-X)
RESET X-INTEREST-RECEIVED-2 (#FR-X)
RESET X-ALT-LP-IND-2        (#FR-X)
RESET X-ALT-LP-AMT-2        (#FR-X)
RESET X-BILL-SW-2           (#FR-X)
RESET X-LPO-PMT-RCVD-2      (#FR-X)
RESET X-LPO-PRIOR-LOAN-BAL-2  (#FR-X)
RESET X-LPO-PRIOR-ACC-INT-2   (#FR-X)
*
RESET #INT
IF ST-V.X-LOAN-AMOUNT (#TO-X) NE 0
  ADD 1 TO #INT
  MOVE EDITED #ORIG-FIXED (EM=ZZZ,ZZZ.99) TO #LOAN-A
  COMPRESS '*' #LOAN-A INTO #LOAN-A LEAVING NO
  MOVE ISSUE-DATE (#FR-X) TO #YYYYMMDD
  COMPRESS #MM-A #DD-A #YYYY-A INTO #ISS-DT-FROM-A WITH '/'
  MOVE ISSUE-DATE (#TO-X) TO #YYYYMMDD
  COMPRESS #MM-A #DD-A #YYYY-A INTO #ISS-DT-TO-A WITH '/'
  COMPRESS #LOAN-A 'Loan transfered from pol' #ISS-DT-FROM-A
           'to' #ISS-DT-TO-A  INTO #NOTE-TEXT (#INT)
END-IF
*
IF ST-V.X-LOAN-AMOUNT-2 (#TO-X) NE 0
  ADD 1 TO #INT
  MOVE EDITED #ORIG-VAR (EM=ZZZ,ZZZ.99) TO #LOAN-A
  COMPRESS '*' #LOAN-A INTO #LOAN-A LEAVING NO
  MOVE ISSUE-DATE (#FR-X) TO #YYYYMMDD
  COMPRESS #MM-A #DD-A #YYYY-A INTO #ISS-DT-FROM-A WITH '/'
  MOVE ISSUE-DATE (#TO-X) TO #YYYYMMDD
  COMPRESS #MM-A #DD-A #YYYY-A INTO #ISS-DT-TO-A WITH '/'
  COMPRESS #LOAN-A 'Loan transfered from pol' #ISS-DT-FROM-A
           'to' #ISS-DT-TO-A  INTO #NOTE-TEXT (#INT)
END-IF
*
IF #INT NE 0
  CALLNAT 'E4205SDM' ST-V.ID-NUMBER #INT #CGA-CLERK
             #NOTE-TEXT(1:10)
END-IF
*
RESET TR-V
MOVE ST-V.PROCESS-IND TO TR-V.PROCESS-IND
MOVE ST-V.ID-NUMBER   TO TR-V.ID-NUMBER
MOVE ST-V.NAME        TO TR-V.NAME
MOVE ST-V.DATE-ACCT-UPDATE  TO TR-V.DATE-LAST-UPDATE
MOVE *INIT-ID         TO TR-V.TERMINAL-ID
MOVE #CGA-CLERK       TO TR-V.CLERK-ID
MOVE *DATN            TO TR-V.TRANSACTION-DATN
MOVE *TIMN            TO TR-V.TRANSACTION-TIMN
*
IF NOT ONLINE-ACTION-GN = '1' OR = '2' OR = '3'
  MOVE '4' TO ONLINE-ACTION-GN
END-IF
*
MOVE 1 TO #ITR1
MOVE ST-V.ISSUE-DATE (#FR-X)  TO #TR-REG-LT-ISSUE-DATE
MOVE EDITED #ORIG-FIXED (EM=ZZ,ZZZ.99) TO #TR-REG-LT-FX-LOAN-AMT
MOVE EDITED #ORIG-VAR (EM=ZZZ,ZZZ.99)  TO #TR-REG-LT-VR-LOAN-AMT
MOVE #TR-REG-LOAN-TRANS TO TR-V.OLD-DATA (#ITR1)
* WRITE 'old TR' TR-V.OLD-DATA (1) (EM=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX)
MOVE ST-V.ISSUE-DATE (#TO-X)  TO #TR-REG-LT-ISSUE-DATE
MOVE EDITED ST-V.X-LOAN-AMOUNT (#TO-X) (EM=ZZ,ZZZ.99)
      TO #TR-REG-LT-FX-LOAN-AMT
MOVE EDITED ST-V.X-LOAN-AMOUNT-2 (#TO-X) (EM=ZZZ,ZZZ.99)
      TO #TR-REG-LT-VR-LOAN-AMT
MOVE #TR-REG-LOAN-TRANS TO TR-V.NEW-DATA (#ITR1)
* WRITE 'new TR' TR-V.NEW-DATA (1) (EM=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX)
*
STORE TR-V
*
UPDATE (0305)
END TRANSACTION
*
MOVE (AD=P) TO #GEN-CV
MOVE 'ST' TO #CGA-SCR-COMMAND
INPUT TEXT 'Transaction Completed' USING MAP 'C2490MLM'
*
FETCH 'G1000PXX'
*
END
